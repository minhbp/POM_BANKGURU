<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="webinaris">

	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
	</listeners>

	<test name="login verify">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.account.Account_Login_Verify" />
		</classes>
	</test>
	
	<test name="actions">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.project.Project_Actions_Setting" />
		</classes>
	</test>
	
	<test name="register">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.project.Registration_Lp" />
		</classes>
	</test>
	
	<test name="upload">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.account.Account_Upload_File" />
		</classes>
	</test>
	
	<test name="clone and copy project">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.account.Account_Clone_Copy_Delete_Project" />
		</classes>
	</test>
	
	<test name="setting email">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.project.Project_Mail_Setting" />
		</classes>
	</test>
	
	<test name="delete projects">
	<parameter name="browser" value="chrome" />
		<classes>
			<class name="queenb.webinaris.account.Account_Delete_Project" />
		</classes>
	</test>
	
		
</suite>